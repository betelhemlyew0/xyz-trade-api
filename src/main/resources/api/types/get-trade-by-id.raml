#%RAML 1.0 DataType

type: object
properties:
  code:
    required: true
    type: string
    example: "AAPL"
  exchange:
    required: true
    type: string
    example: "NYSE"
  operation:
    required: true
    enum: ["buy", "sell"]
    example: "buy"
  price:
    required: true
    type: number
    example: 233
  quantity:
    required: true
    type: number
    example: 4
      
      

